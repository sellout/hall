{-| Turns a list of dependency versions into a Cabal-style dependency range.
-}
let P =
      https://prelude.dhall-lang.org/v20.1.0/package.dhall
        sha256:26b0ef498663d269e4dc6a82b0ee289ec565d683ef4c00d0ebdd25333a5a3c98

let Pvp = ../PVP/package.dhall

let Dependency = ../hpack/Dependency/Type

let buildPvpDependency = ./buildPvpDependency

in  P.List.map
      (P.Map.Entry Text (List Pvp.Version))
      Dependency
      ( λ(entry : P.Map.Entry Text (List Pvp.Version)) →
          buildPvpDependency entry.mapKey entry.mapValue
      )
