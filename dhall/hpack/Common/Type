let P =
      https://prelude.dhall-lang.org/v20.1.0/package.dhall
        sha256:26b0ef498663d269e4dc6a82b0ee289ec565d683ef4c00d0ebdd25333a5a3c98

let Conditional = ../Conditional/Type

let Dependency = ../Dependency/Type

in  λ(a : Type) →
      { buildable : Bool
      , source-dirs : List Text
      , default-extensions : List Text
      , other-extensions : List Text
      , ghc-options : List Text
      , ghc-prof-options : List Text
      , ghcjs-options : List Text
      , cpp-options : List Text
      , cc-options : List Text
      , c-sources : List Text
      , cxx-options : List Text
      , cxx-sources : List Text
      , js-sources : List Text
      , extra-lib-dirs : List Text
      , extra-libraries : List Text
      , include-dirs : List Text
      , frameworks : List Text
      , extra-frameworks-dirs : List Text
      , ld-options : List Text
      , dependencies : P.Map.Type Text Dependency
      , pkg-config-dependencies : P.Map.Type Text Dependency
      , build-tools : List Text
      , system-build-tools : List Text
      , when : List (Conditional a)
      }
