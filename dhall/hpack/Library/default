let Common = ../Common/package.dhall

let DefaultType = ./DefaultType

in  λ(a : Type) →
          Common.default a
        ∧ { exposed = True
          , exposed-modules = [] : List Text
          , generated-exposed-modules = [] : List Text
          , other-modules = [] : List Text
          , generated-other-modules = [] : List Text
          , reexported-modules = [] : List Text
          , signatures = [] : List Text
          }
      : DefaultType a
